prefix=/usr/ppc-amigaos/SDK/local/newlib
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: cairo
Description: Multi-platform 2D graphics library
Version: 1.14.10

Requires.private:         pixman-1 >= 0.30.0    freetype2 >= 9.7.3   libpng 
Libs: -L${libdir} -lcairo
Libs.private:           -lz -lz    -lauto
Cflags: -I${includedir}/cairo
